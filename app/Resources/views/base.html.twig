<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}TFM - Red Social{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        {% block stylesheets %}
            <link href="{{ asset('bundles/app/vendor/bootstrap/css/bootstrap.css') }}" type="text/css"  rel="stylesheet" />
            <link href="{{ asset('bundles/app/vendor/fontawesome/css/font-awesome.min.css') }}" type="text/css"  rel="stylesheet" />

            <link href="{{ asset('bundles/app/css/custom.css') }}" type="text/css"  rel="stylesheet" />

        {% endblock %}
        {% block javascripts %}
            <script src="{{ asset('bundles/app/vendor/jquery/jquery1.9.1.js') }}"></script>
            <script src="https://npmcdn.com/tether@1.2.4/dist/js/tether.min.js"></script>
            <script src="{{ asset('bundles/app/vendor/bootstrap/js/bootstrap.js') }}"></script>
            <script src="{{ asset('bundles/profile/vendor/notifyjs/notify.js') }}"></script>
            <script src="{{ asset('bundles/profile/js/Friends.js') }}"></script>
            <script src="{{ asset('bundles/app/js/Spotify.js') }}"></script>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block header %}

        {% endblock %}
        {% block body %}

        {% endblock %}

    </body>
</html>
